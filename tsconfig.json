{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],"@/*": [
        "./src/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}